{
    "name": "ping2me/laravel",
    "description": "Laravel client for Ping2.me",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ping2Me\\Laravel\\": "src/",
            "Ping2Me\\Laravel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "bangnokia",
            "email": "bangnokia@gmail.com"
        }
    ],
    "require": {
        "php": "^7|^8",
        "ping2me/php": "^0.2",
        "illuminate/contracts": "^10.37",
        "illuminate/support": "^10.37"
    },
    "require-dev": {
        "orchestra/testbench": "^8.18",
        "pestphp/pest": "^2.28"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ping2Me\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
