{
    "name": "ping2me/laravel",
    "description": "Laravel client for Ping2.me",
    "type": "library",
    "license": "MIT",
    "keywords": ["ping2me", "laravel", "ping2.me", "notification", "telegram"],
    "autoload": {
        "psr-4": {
            "Ping2Me\\Laravel\\": "src/",
            "Ping2Me\\Laravel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "bangnokia",
            "email": "bangnokia@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "ping2me/php": "^0.3",
        "illuminate/contracts": "^11.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ping2Me\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
